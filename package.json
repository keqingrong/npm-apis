{
  "name": "@keqingrong/npm-apis",
  "version": "0.3.0",
  "description": "Programmatic APIs for npm",
  "module": "dist/npm-apis.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "npm",
    "libnpm"
  ],
  "author": "Qingrong Ke <keqingrong@outlook.com> (https://keqingrong.github.io/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keqingrong/npm-apis.git"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "decamelize": "^5.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/node": "^15.0.2",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "prettier": "^2.3.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/npm-apis.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/npm-apis.esm.js",
      "limit": "10 KB"
    }
  ]
}
